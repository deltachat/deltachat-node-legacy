  "bugs": {
    "url": "https://github.com/karissa/deltachat-node/issues"
  },
  "homepage": "https://github.com/karissa/deltachat-node#readme",
  "dependencies": {
    "nan": "^2.4.0",
    "node-gyp-build": "^3.0.0",
    "threads": "^0.12.0"
  },
  "devDependencies": {
    "node-gyp": "^3.6.1",
    "tape": "^4.8.0"
  }
}
